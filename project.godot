; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Blink Odyssey"
run/main_scene="uid://b2p2dxp136jar"
config/features=PackedStringArray("4.4", "GL Compatibility")

[autoload]

DataManager="*res://Scripts/Managers/data_manager.gd"
ComponentsManager="*res://Scripts/Managers/components_manager.gd"
FormatManager="*res://Scripts/Managers/format_manager.gd"
SceneManager="*res://Scripts/Managers/scene_manager.gd"

[display]

window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_resource_groups/plugin.cfg")

[file_customization]

folder_colors={
"res://Assets/": "red",
"res://Data/": "pink",
"res://Scenes/": "blue",
"res://Scripts/": "teal"
}

[godot_resource_groups]

auto_rebuild=true

[gui]

theme/custom_font="uid://dt2r8hvmcxm2"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
